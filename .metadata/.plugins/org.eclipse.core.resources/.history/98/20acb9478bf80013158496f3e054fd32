package
{
	public class StepInfo
	{
		private var _must:int;
		
		private var _maybe:Array;
		
		public function StepInfo( must:int, maybe:Array )
		{
			this._must = must;
			this._maybe = maybe;
		}
		
		public function setMust( val:int ):void{
			this._must = val;
		}
		
		public function setMaybe( arr:Array ):void{
			this._maybe = arr;
		}
	}
}