package
{
	public class StepTimeline
	{
		private var _line:Array;
		
		private var _currIndex;
		
		public function StepTimeline()
		{
			_line = [];
			_currIndex = 0;
		}
		
		public function pushStep( step:StepInfo ):void{
			_line.push( step );
		}
		
		public function getCurrStep():StepInfo{
			
		}
	}
}